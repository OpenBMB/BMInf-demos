        "element-ui": "2.13.2",
        "eslint": "^7.32.0",
        "gulp-clean-css": "^4.3.0",
        "gulp-css-wrap": "^0.1.2",
        "jquery": "^3.5.1",
        "js-cookie": "2.2.0",
        "moment": "^2.27.0",
        "normalize.css": "7.0.0",
        "nprogress": "0.2.0",
        "path-to-regexp": "2.4.0",
        "qs": "^6.9.4",
        "tinymce": "^5.9.1",
